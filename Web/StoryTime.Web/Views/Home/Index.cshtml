@model StoryTime.Web.ViewModels.Home.IndexViewModel
@{
    ViewBag.Title = "Home Page";
}
<div class="row">
    <div class="col-md-6">
        <h1>Stories</h1>
        @Html.DisplayFor(m => m.Stories)
    </div>
    <div class="col-md-6">
        <h1>Template Stories</h1>
        @Html.DisplayFor(m => m.TemplateStories)
    </div>
</div>

@section jokes{
    <div class="alert alert-dismissible alert-info">
        <button type="button" class="close" data-dismiss="alert">X</button>

    </div>
}